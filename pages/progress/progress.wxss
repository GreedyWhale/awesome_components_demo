@import "../../assets/wxss/common.wxss";

.color-btns {
  margin-top: 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
}
.color-btns-item {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.color-btns-box {
  width: 100rpx;
  height: 100rpx;
  border-radius: 8rpx;
  margin-bottom: 10rpx;
}
.dialog-content {
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 20rpx;
}
.dialog-content-item {
  color: #fff;
  text-align: center;
  flex: 0 0 30%;
  font-size: 24rpx;
  margin-bottom: 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 8rpx;
}