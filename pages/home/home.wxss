@import "../../assets/wxss/icon.wxss";

page {
  background: #f0d22e;
  padding: 0 40rpx 20rpx;
  box-sizing: border-box;
}
.header {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 20rpx 0;
}
.header-icon {
  width: 100rpx;
  margin-bottom: 40rpx;
}
.item {
  display: flex;
  align-items: center;
  padding: 20rpx;
  border: 2rpx solid #161616;
  margin-bottom: 40rpx;
  border-radius: 8rpx;
}
.item-icon {
  width: 100rpx;
  height: 100%;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 64rpx;
}
.item-icon .iconfont {
  font-size: 64rpx;
}
.item-info {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}
.item-info-title {
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.item-info-desc {
  font-size: 24rpx;
  font-weight: thin;
}
.item-go-next-btn.iconfont {
  font-size: 50rpx;
}